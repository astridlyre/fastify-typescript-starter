    "fastify-helmet": "^7.0.1",
    "fastify-jwt": "^4.1.3",
    "fastify-rate-limit": "^5.7.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
